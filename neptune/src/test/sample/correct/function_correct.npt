//correct function calls
function int square(int c){
	return(c*c);
}

function int fibo(int n){
	int result = 1;
	if(n <= 1){
		result = 1;
	}else{
		int a = fibo(n-1);
		int b = fibo(n-2);
		result = a + b;
	} 
	return(result);
}



function char identity(char a){
	return(a);
}

function bool inverse(bool b){
	return(!b);
}

function bool nand(bool a, bool b){
	return(!(a && b));
}

