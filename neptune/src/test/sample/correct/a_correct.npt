// commentaar in een programma voorbeeld
function int fibo(int n){
	int result = 0;
	if(n <= 2){
		result = 1;
	}else{
		int a = fibo(n-1);
		int b = fibo(n-2);
		result = a + b;
	} 
	return(result);
}

int a;
read(a);
int i = 0;
char newLine = 10;

if(a > 0) {

	while(i <= a) {
		print('(', i, ")  ", fibo(i), newLine);
		i = i + 1;
	}

}else{
	print("Negatieve waarde is niet toegestaan");
}