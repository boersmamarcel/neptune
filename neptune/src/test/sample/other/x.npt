function int square(int c){
	return(c*c);
};

char newline = 10;

print(square(2));
print(newline);
print(square(3));

function int fibo(int n){

	int result = 0;
	if(n==1){
		result = 1;
	}elsif(n > 1){
		int a = fibo(n-2);
		int b = fibo(n-1);
		result = a+b;
	}
	
	return(result);
};


print(fibo(0));
print(newline);
print(fibo(1));
print(newline);
print(fibo(2));
print(newline);
print(fibo(3));
print(newline);
print(fibo(4));
print(newline);
print(fibo(5));
print(newline);